# Required for Gemini intake via Datapizza
GEMINI_API_KEY=

# Optional configuration
TRIPPLANNER_GEMINI_MODEL=gemini-2.5-flash
TRIPPLANNER_DEMO_OFFLINE=1
TRIPPLANNER_TIMEZONE=UTC
TRIPPLANNER_NOW_TS=     # current time, if unset it uses the timestamp
TRIPPLANNER_OUTPUT_LANGUAGE=en    # accept en, it

# Telemetry
TRIPPLANNER_TRACING_ENABLED=1
TRIPPLANNER_TRACING_EXPORTER=console
TRIPPLANNER_OTLP_ENDPOINT=http://localhost:4318/v1/traces
TRIPPLANNER_OTLP_TIMEOUT_MS=1000
